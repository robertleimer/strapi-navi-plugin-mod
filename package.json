{
  "name": "simple-strapi-navigation-plugin",
  "version": "1.0.14",
  "description": "Strapi - Simple Navigation plugin",
  "strapi": {
    "name": "navigation",
    "displayName": "Simple Navigation",
    "description": "Create an consumable navigation list with a simple and straightforward visual builder (no url needed)",
    "kind": "plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertleimer/strapi-navi-plugin-mod"
  },
  "scripts": {
    "publish:latest": "cd build && npm publish --tag latest",
    "prepublish:latest": "npm run clean && npm run build && node build/setup-package.js",
    "test:unit": "jest --verbose --coverage",
    "test:unit:watch": "jest --watch",
    "build": "tsc",
    "build:dev": "npm run build && cp ./package.json ./build && cd ./build && yarn",
    "clean": "rmdir build",
    "develop": "nodemon --exec \"npm run build:dev\""
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@sindresorhus/slugify": "1.1.0",
    "@strapi/utils": "^4.20.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "6.4.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@jest/types": "29.5.x",
    "@strapi/helper-plugin": "4.20.0",
    "@strapi/admin": "4.20.0",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.14.181",
    "@types/pluralize": "^0.0.29",
    "@types/uuid": "^8.3.4",
    "codecov": "^3.7.2",
    "formik": "^2.2.9",
    "jest": "29.6.0",
    "jest-cli": "^27.5.1",
    "jest-styled-components": "^7.0.2",
    "koa": "^2.8.0",
    "nodemon": "^2.0.15",
    "strapi-typed": "1.0.17",
    "ts-jest": "29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@strapi/data-transfer": "4.15.5",
    "@strapi/strapi": "4.x",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "author": {
    "name": "RobertLeimer",
    "email": "robert.leimer@proton.me"
  },
  "maintainers": [
    {
      "name": "Robert Leimer",
      "email": "robert.leimer@proton.me"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "./build/**/*"
    ],
    "ext": "js,json,ts"
  },
  "keywords": [
    "strapi",
    "plugin",
    "navigation"
  ],
  "license": "MIT"
}
